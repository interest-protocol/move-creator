{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"],
      "@/svg": ["./src/components/svg"],
      "@/components": ["./components/terminal/components"],
      "@/components/*": ["./components/terminal/components/*"],
      "@/context/*": ["./components/terminal/context/*"],
      "@/constants": ["./components/terminal/constants"],
      "@/constants/*": ["./components/terminal/constants/*"],
      "@/interface": ["./components/terminal/interface"],
      "@/lib": ["./components/terminal/lib"],
      "@/lib/*": ["./components/terminal/lib/*"],
      "@/hooks": ["./components/terminal/hooks"],
      "@/hooks/*": ["./components/terminal/hooks/*"],
      "@/utils": ["./components/terminal/utils"],
      "@/utils/*": ["./components/terminal/utils/*"],
      "@/integration/*": ["./components/terminal/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
